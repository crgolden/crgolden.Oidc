{
  "CorsOptions": {
    "Origins": [
      "https://clarity-ui.azurewebsites.net",
      "https://clarity-api.azurewebsites.net"
    ]
  },
  "DatabaseOptions": {
    "SeedData": "false",
    "DatabaseType": "SqlServer",
    "SqlServerOptions": {
      "ConnectTimeout": "30",
      "DataSource": "tcp:clarity-server.database.windows.net,1433",
      "Encrypt": "true",
      "InitialCatalog": "clarity-db",
      "IntegratedSecurity": "false",
      "MultipleActiveResultSets": "false",
      "PersistSecurityInfo": "false",
      "TrustServerCertificate": "false"
    }
  },
  "IdentityServerAddress": "https://clarity-oidc.azurewebsites.net",
  "KeyVaultName": "clarity-oidc-key-vault"
}
