@page "{userCode}"
@model IndexModel

@{
    ViewData["Title"] = "Device Flow Code Details";
}

<div class="row">
    <div class="col-12 col-md-6">
        <dl>
            <dt>
                @Html.LabelFor(model => model.DeviceFlowCode.UserCode, "User Code")
            </dt>
            <dd>
                @Html.DisplayFor(model => model.DeviceFlowCode.UserCode)
            </dd>
            <dt>
                @Html.LabelFor(model => model.DeviceFlowCode.DeviceCode, "Device Code")
            </dt>
            <dd>
                @Html.DisplayFor(model => model.DeviceFlowCode.DeviceCode)
            </dd>
            <dt>
                @Html.LabelFor(model => model.DeviceFlowCode.CreationTime, "Created")
            </dt>
            <dd>
                @Html.DisplayFor(model => model.DeviceFlowCode.CreationTime)
            </dd>
            <dt>
                @Html.LabelFor(model => model.DeviceFlowCode.Expiration, "Expires")
            </dt>
            <dd>
                @Html.DisplayFor(model => model.DeviceFlowCode.Expiration)
            </dd>
        </dl>
    </div>
    <div class="col-12 col-md-6">
        <dl>
            <dt>
                @Html.LabelFor(model => model.DeviceFlowCode.SubjectId, "Subject Id")
            </dt>
            <dd>
                @Html.DisplayFor(model => model.DeviceFlowCode.SubjectId)
            </dd>
            <dt>
                @Html.LabelFor(model => model.DeviceFlowCode.ClientId, "Client Id")
            </dt>
            <dd>
                @Html.DisplayFor(model => model.DeviceFlowCode.ClientId)
            </dd>
            <dt>
                @Html.DisplayNameFor(model => model.DeviceFlowCode.Data)
            </dt>
            <dd>
                @Html.DisplayFor(model => model.DeviceFlowCode.Data)
            </dd>
        </dl>
    </div>
</div>
<hr/>
<div class="row">
    <div class="col">
        <h4>
            <a asp-page="../Index" title="Device Flow Codes">
                <i class="fal fa-list"></i> List |
            </a>
            <a asp-page="../Delete" asp-route-userCode="@Model.DeviceFlowCode.UserCode" title="Delete Device Flow Code">
                <i class="fal fa-trash"></i> Delete
            </a>
        </h4>
    </div>
</div>
<hr/>
